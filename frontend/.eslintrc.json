{
    "extends": ["eslint:recommended", "prettier"],
    "plugins": ["react"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "requireConfigFile": false,
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    }
  }